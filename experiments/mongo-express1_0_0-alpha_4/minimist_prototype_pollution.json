                    "name": "http_probe",
                    "success": "status_code == 200"
                },
                "http": {
                    "url": "http://your-service/health",
                    "method": "GET"
                }
            }
        ]
    },
    "method": [
        {
            "type": "http",
            "name": "http_attack",
            "http": {
                "url": "http://your-service/vulnerable-endpoint",
                "method": "POST",
                "headers": {
                    "Content-Type": "application/json"
                },
                "body": {
                    "key": "__proto__",
                    "value": "polluted_value"
                }
            }
        }
    ],
    "rollbacks": [],
    "duration": 60,
    "retries": 3
}